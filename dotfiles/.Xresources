! Xresources file

! transparency
!URxvt.transparent:      true
!URxvt.shading:          30

! turn off scrollbar
URxvt.scrollBar:        off

! font
!URxvt.font:             -*-profont-*-*-*-*-12-*-*-*-*-*-*-*
URxvt.font:             xft:profont:size=8

! open URLs in chrome
URxvt.perl-ext-common:  default,matcher,vtwheel
URxvt.urlLauncher:      /usr/bin/chromium
URxvt.matcher.button:   1

! make URLs funny colours
URxvt.colorUL: #4682B4

! set 256 colors term
URxvt*termName:         screen-256color

! colours
URxvt.color0:           #000000
URxvt.color1:           #A80000
URxvt.color2:           #00A800
URxvt.color3:           #A8A800
URxvt.color4:           #0000A8
URxvt.color5:           #A800A8
URxvt.color6:           #00A8A8
URxvt.color7:           #A8A8A8

URxvt.color8:           #0073E6
URxvt.color9:           #FF0054
URxvt.color10:          #00FF54
URxvt.color11:          #FFFF54
URxvt.color12:          #9ECFFF
URxvt.color13:          #FF00FF
URxvt.color14:          #00FFFF
URxvt.color15:          #FFFFFF

URxvt.cursorColor:      #E0E2E4
URxvt.pointerColor:     #E0E2E4
URxvt.foreground:       #E0E2E4
URxvt.background:       #293134
